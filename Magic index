import java.util.*;
class magic_index
{
	public static void main(String args[])
	{
		Scanner s=new Scanner(System.in);
		int t=s.nextInt();
		int sum=0;
		while(t-->0)
		{
			int n=s.nextInt();
			int a[]=new int[n];
			for(int i=0;i<n;i++)
			{
				a[i]=s.nextInt();
			}
			for(int j=0;j<n;j++)
			{
				if(a[j]==j)
				{
					sum=a[j];
					break;
				}
				else
				{
					sum=-1;
				}
			}
			System.out.println();
			System.out.println(sum);
		}
	}
}
